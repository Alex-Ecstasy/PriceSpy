@model SampleViewModel
@{
    ResponseContent responseContent = new();
}
            <div class="accordion" id="accordionPanelsStayOpenExample">

            @if (!string.IsNullOrEmpty(SampleViewModel.TextInfo))
            {
                <h6>@SampleViewModel.TextInfo</h6>
            }
            else
            {
                <p>Результатов в прайсах: @SampleViewModel.TotalCount</p>
            }

                @try
                {
                    @foreach (var selectedShippers in Model.Shippers)
                    {
                        @await Html.PartialAsync("Accordion", selectedShippers)
                    }
                }
            catch (Exception ex)
            {
                Console.WriteLine(ex);
            }
            </div>

